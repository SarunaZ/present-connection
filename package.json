{
  "name": "present-connection",
  "version": "1.0.1",
  "description": "A mini-project showcase for present connection front-end academy",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@types/node": "12.20.36",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.0",
    "autoprefixer": "10.4.0",
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.1.2",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "typescript": "4.1.6"
  },
  "scripts": {
    "build:css": "postcss src/Styles/main.css -o src/index.css",
    "watch:css": "postcss src/Styles/main.css -o src/index.css -w",
    "start": "npm-run-all -p watch:css start-react",
    "build": "npm-run-all build:css build-react",
    "start-react": "react-scripts start",
    "build-react": "react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      "since 2010",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "since 2010"
    ]
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "postcss": "8.3.11",
    "postcss-assets": "6.0.0",
    "postcss-cli": "9.0.2",
    "postcss-import": "14.0.2",
    "postcss-nested": "5.0.6"
  }
}
